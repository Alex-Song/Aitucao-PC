<Window x:Class="DanmakuChi.QRCode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DanmakuChi"
        mc:Ignorable="d"
        Title="QRCode" Width="360" Height="360">
    <Grid>
        <Image x:Name="imgBox" Margin="20,20,20,40"/>
        <TextBox x:Name="textBox" Margin="20,0,20,10" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Bottom" Height="24" VerticalContentAlignment="Center"/>

    </Grid>
</Window>
